<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.AttendanceFragment">

    <TextView
        android:id="@+id/ltv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Week"
        android:layout_marginStart="60dp"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
       />

    <TextView
        android:id="@+id/ltv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="10dp"
        android:text="Attendance"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="40dp"
        app:layout_constraintTop_toTopOf="parent"

        />


    <Button
        android:id="@+id/Week1"
        android:layout_width="62dp"
        android:layout_height="70dp"
        app:layout_constraintTop_toBottomOf="@id/ltv1"
        app:layout_constraintStart_toStartOf="@id/ltv1"
        app:strokeColor="@color/orange"
        app:strokeWidth="3dp"
        app:cornerRadius="30dp"
        android:backgroundTint="@color/white"
        android:text="1"
        android:textSize="25sp"
        android:textStyle="bold"

        />

    <Button
        android:id="@+id/Week2"
        android:layout_width="62dp"
        android:layout_height="70dp"
        app:layout_constraintTop_toBottomOf="@id/Week1"
        app:layout_constraintStart_toStartOf="@id/Week1"
        app:strokeColor="@color/orange"
        app:strokeWidth="3dp"
        app:cornerRadius="30dp"
        android:backgroundTint="@color/white"
        android:text="2"
        android:textSize="25sp"
        android:textStyle="bold"/>

    <Button
        android:id="@+id/Week3"
        android:layout_width="62dp"
        android:layout_height="70dp"
        app:layout_constraintTop_toBottomOf="@id/Week2"
        app:layout_constraintStart_toStartOf="@id/Week1"
        app:strokeColor="@color/orange"
        app:strokeWidth="3dp"
        app:cornerRadius="30dp"
        android:backgroundTint="@color/white"
