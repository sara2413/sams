    <TextView
        android:id="@+id/tv33"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="25dp"
        android:fontFamily="@font/micross"
        android:text="sara shaheen sara hamed sara shaheen sara hamed "
        android:textColor="@color/noty"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView41" />

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="59dp"
        android:background="#707070"
        android:layout_marginEnd="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tv44"
        app:layout_constraintEnd_toEndOf="@id/tv44"
        app:layout_constraintTop_toBottomOf="@id/tv44"

